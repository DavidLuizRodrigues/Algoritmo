algoritmo "semnome"

var
   a, nasc: inteiro
inicio
      escreva("Ano Atual: ")
      leia(a)
      escreva("Ano de Nascimento: ")
      leia(nasc)
      se a-nasc>=18 entao
         escreva("Você está apto para dirigir")
      senao
         escreva("Você não está apto para dirigir")
      fimse

fimalgoritmo
