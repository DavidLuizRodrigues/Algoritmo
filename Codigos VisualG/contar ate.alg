algoritmo "conteate0"
var
   contador, c: inteiro
inicio
      escreva("Deseja contar até quanto? ")
      leia(c)
      contador<-0
      enquanto (contador<=c) faca
               escreval(contador)
               contador<-contador+1
      fimenquanto
      escreval("Terminei de contar")
fimalgoritmo
